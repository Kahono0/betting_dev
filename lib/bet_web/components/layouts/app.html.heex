<header class="px-4 sm:px-6 lg:px-8">
  <div class="flex items-center justify-between py-6 px-2 text-md lg:text-xl mt-4">
    <div class="flex items-center space-x-4">
      <a href="/" class="font-bold text-zinc-900 flex justify-center items-center">
        Bet <img src="data:image/jpeg;base64," alt="Bet" class="w-8 h-8" />
      </a>
      <a href="/dashboard" class="text-zinc-600 hover:text-zinc-900">Matches</a>
      <a href="/users" class="text-zinc-600 hover:text-zinc-900">Bets</a>
    </div>
    <div class="flex items-center space-x-4">
      <a href="/profile" class="text-zinc-600 hover:text-zinc-900">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          stroke-width="1.5"
          stroke="currentColor"
          class="w-6 h-6"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            d="M17.982 18.725A7.488 7.488 0 0 0 12 15.75a7.488 7.488 0 0 0-5.982 2.975m11.963 0a9 9 0 1 0-11.963 0m11.963 0A8.966 8.966 0 0 1 12 21a8.966 8.966 0 0 1-5.982-2.275M15 9.75a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z"
          />
        </svg>
      </a>
      <a href="/logout" class="text-zinc-600 hover:text-zinc-900">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          stroke-width="1.5"
          stroke="currentColor"
          class="w-6 h-6"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            d="M8.25 9V5.25A2.25 2.25 0 0 1 10.5 3h6a2.25 2.25 0 0 1 2.25 2.25v13.5A2.25 2.25 0 0 1 16.5 21h-6a2.25 2.25 0 0 1-2.25-2.25V15M12 9l3 3m0 0-3 3m3-3H2.25"
          />
        </svg>
      </a>
    </div>
  </div>
</header>
<main class="px-4 py-20 sm:px-6 lg:px-8">
  <div class="mx-auto max-w-2xl">
    <.flash_group flash={@flash} />
    <%= @inner_content %>
  </div>
</main>
